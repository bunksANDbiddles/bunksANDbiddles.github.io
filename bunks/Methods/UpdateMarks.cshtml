@{
    if(IsPost){
        var db = Database.Open("BunksAndBiddlesSchool");
        var sql = @"UPDATE PaceResult SET subjectName = @0, paceNo = @1, admissionNo = @2, score = @3, 
                    studentID = @4, SupervisorID = @5, teacherName = @6 WHERE paceResultID = @7";
          var paceResultID = Request["paceResultID"];
        var subjectName = Request["subjectName"];
        var paceNo = Request["paceNo"];
        var admissionNo = Request["admissionNo"];
        var score = Request["score"];
        var studentID = Request["studentID"];
        var SupervisorID = Request["SupervisorID"];
        var teacherName = Request["teacherName"];
      
        db.Execute(sql, paceResultID, subjectName, paceNo, admissionNo, score, studentID, SupervisorID, teacherName);
    }
}
