@{
    
  if(IsPost){

         var extraCurricularTitle = "";
        var comment = "";
     
      var admissionNo = "";
     var teacherName = "";
     
      var SupervisorID = "";
      var pupilID = "";

  var db = Database.Open("BunksAndBiddlesSchool"); 
     var orderSql = "INSERT INTO KinderExtraCurricularReport ( extraCurricularTitle,  admissionNo,SupervisorID, pupilID, comment,teacherName)" +
                                "VALUES (@0, @1, @2, @3, @4, @5)";


      extraCurricularTitle = Request["extraCurricularTitle"];
     comment = Request["comment"];
     admissionNo = Request["admissionNoReport"];
     teacherName =Request["teacherNameReport"];
     SupervisorID =Request["supervisorIDReport"];
     pupilID = Request["studentIDReport"];
       
      


     db.Execute( orderSql,  extraCurricularTitle, admissionNo, SupervisorID,  pupilID, comment,teacherName); 
     
     
     
 
     }


     

